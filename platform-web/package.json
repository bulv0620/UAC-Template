{
  "type": "module",
  "name": "v3-admin-vite",
  "version": "4.2.4",
  "description": "一个免费开源的中后台管理系统基础解决方案，基于 Vue3、TypeScript、Element Plus、Pinia 和 Vite 等主流技术",
  "author": {
    "name": "pany",
    "email": "939630029@qq.com",
    "url": "https://github.com/pany-ang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/un-pany/v3-admin-vite.git"
  },
  "scripts": {
    "dev": "vite",
    "build:stage": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,tests,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "test": "vitest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.2",
    "@imengyu/vue3-context-menu": "^1.3.3",
    "axios": "1.6.0",
    "dayjs": "1.11.10",
    "element-plus": "2.11.5",
    "js-cookie": "3.0.5",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "monaco-editor": "^0.54.0",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "8.3.0",
    "pinia": "3.0.3",
    "screenfull": "6.0.2",
    "uuid": "^13.0.0",
    "vue": "3.5.22",
    "vue-router": "4.2.5",
    "vuedraggable": "^4.1.0",
    "xe-utils": "3.5.11"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.6",
    "@types/lodash-es": "4.17.11",
    "@types/node": "20.9.0",
    "@types/nprogress": "0.2.3",
    "@types/path-browserify": "1.0.2",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/test-utils": "2.4.1",
    "eslint": "9.38.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "10.5.1",
    "jsdom": "27.0.1",
    "lint-staged": "16.2.6",
    "prettier": "3.0.3",
    "sass": "1.93.2",
    "typescript": "5.2.2",
    "vite": "7.1.12",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vitest": "4.0.1",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.1.1"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "vue-admin",
    "vue3-admin",
    "vite",
    "vite-admin",
    "element-plus",
    "element-plus-admin",
    "ts",
    "typescript"
  ],
  "license": "MIT"
}
